<cache>
   <cache-entry>
      <class>servlet</class>
      <name>web/_style</name>
      <cache-id>
         <component id="include" type="parameter">
            <required>true</required>
         </component>
         <component id="etag" type="parameter">
            <required>true</required>
         </component>
         <timeout>1800</timeout>
      </cache-id>
   </cache-entry>
   <cache-entry>
      <class>servlet</class>
      <name>web/_js/</name>
      <name>web/_js</name>
      <cache-id>
         <component id="include" type="parameter">
            <required>true</required>
         </component>
         <component id="exclude" type="parameter">
            <required>false</required>
         </component>
         <component id="etag" type="parameter">
            <required>true</required>
         </component>
         <component id="_proxyURL" type="parameter">
            <required>true</required>
         </component>
         <component id="lang" type="parameter">
            <required>false</required>
         </component>
         <component id="country" type="parameter">
            <required>false</required>
         </component>
         <component id="variant" type="parameter">
            <required>false</required>
         </component>
         <component id="debug" type="parameter">
            <required>false</required>
         </component>
         <component id="_localeOverride" type="parameter">
            <required>false</required>
         </component>
         <component id="xdloader" type="parameter">
            <required>false</required>
         </component>
         
         <timeout>1800</timeout>
      </cache-id>
   </cache-entry>
</cache> 
