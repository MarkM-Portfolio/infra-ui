<?eclipse version="3.4"?>
<plugin>
   <extension
         point="net.jazz.ajax.webBundles">
      <alias
            value="lconn.oauth">
      </alias>
   </extension>
   <extension
         point="net.jazz.ajax.dojoModules">
      <dojoModule
            id="lconn.oauth.nls.ui"
            provider="com.ibm.lconn.oauth.web.resources.MessageBundles">
      </dojoModule>
      <dojoModule
            id="lconn.oauth.nls.uihelp"
            provider="com.ibm.lconn.oauth.web.resources.MessageBundles">
      </dojoModule>
      <dojoModule
            id="lconn.oauth.config"
            provider="com.ibm.lconn.oauth.web.resources.config.OAuthConfig">
      </dojoModule>
   </extension>

</plugin>
