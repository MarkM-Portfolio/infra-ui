<component name="lc.web.resources.base" 
            owner="Clayton Coleman" 
            owner-email="claycole@us.ibm.com"
            project-type="eclipse-pde">

    <!--An output group is needed to get it into the fe.zip -->
    <output-group name="to_osgi_war">
       <output type="dir" 
               visibility="public" 
               platform="all" 
               location="${env.LWP_COMP_BUILD}/eclipse" 
               preferredInstallLocation="WEB-INF/eclipse"/>
    </output-group>

    <output-group name="to_jsdoc">
       <output type="dir" 
               visibility="public" 
               platform="all" 
               location="${env.LWP_COMP_BUILD}/jsdoc" 
               preferredInstallLocation="jsdoc"/>
    </output-group>
	
   <!-- net.jazz.ajax is compiled based on these resources -->
   <dependency name="prereqs.web.resources" type="dir" />
   <dependency name="dojo.web.resources" type="dir" />
   <dependency name="lc.web.osgi" type="dir" />
   
   <!--dependency name="prereqs.sn/jsdoc3" type="dir" /-->
   <dependency name="prereqs.sn/jslint4java" type="dir" />
</component>
