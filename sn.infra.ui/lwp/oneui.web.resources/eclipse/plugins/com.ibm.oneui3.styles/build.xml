<project name="com.ibm.oneui3.styles" default="all" basedir=".">
	<taskdef resource="net/sf/antcontrib/antcontrib.properties"/>
	<property name="oneui.dir" value="${basedir}/resources" />
	<property name="srcDir" value="${oneui.dir}/imageLibrary" />
	<property name="outDir" value="${oneui.dir}/css/images" />

	<!-- Macro for Sprite Generation -->
	<macrodef name="spritegen">
		<attribute name="srcPath" />
		<attribute name="srcProperties" />
		<attribute name="outPath" />
		<attribute name="outName" />
		<sequential>
			<!-- Sprite the images defined in the srcProperties file at the srcPath, and create the sprite output at outPath with outName -->
			<java dir="@{srcPath}" fork="true" jvm="${java.home}/bin/java" classpath="${basedir}/tools/spritegen/oneuiSpriteGenerator.jar" classname="com.ibm.oneui.spritegen.tool.SpriteGeneratorTool">
				<arg value="name=@{outName}" />
				<arg value="srcdir=@{srcPath}" />
				<arg value="srclist=@{srcPath}/@{srcProperties}" />
				<arg value="outdir=@{outPath}" />
				<arg value="outpng32=@{outName}-8bit.png" />
				<arg value="rtimgpath=." />
				<arg value="outcss=@{outName}.css" />
				<arg value="outhtml=@{outName}-test.html" />
				<arg value="outdim=@{outName}.properties" />
				<arg value="pady=8" />
				<arg value="verbose=false" />
				<arg value="copyright=${basedir}/copyright.txt" />
			</java>

			<!-- If Windows run the NPNGQuant.exe tool to quantize the 32-bit sprite output to an 8-bit sprite -->
			<!--<if>
 				<os family="windows" />
 				<then>
					<move file="@{outPath}/@{outName}-8bit.png" tofile="@{outPath}/@{outName}-32bit.png" />
					<exec executable="${basedir}/tools/NPNGQuant.exe" dir="@{outPath}">
						<arg value="-split" />
						<arg value="-force" />
						<arg value="-nofs" />
						<arg value="-verbose" />
						<arg value="256" />
						<arg value="@{outName}-32bit.png" />
					</exec>
					<move file="@{outPath}/@{outName}-32bit-or8.png" tofile="@{outPath}/@{outName}-8bit.png" />
				</then>
				Placeholder for Unix systems, once a similar tool has been approved add it below
 				<else>
   					<echo message="Image Quantization only supported on Windows." />
 				</else>
			</if> -->
		</sequential>
	</macrodef>

