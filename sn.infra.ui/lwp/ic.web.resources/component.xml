<component name="ic.web.resources" 
            owner="Claudio Procida" 
            owner-email="procidac@ie.ibm.com"
            project-type="eclipse-pde">

    <!-- All contents of ic.web.resources are by default added to the web resources WAR -->
    <output-group name="to_osgi_war">
       <output type="dir" 
               visibility="public" 
               platform="all" 
               location="${env.LWP_COMP_BUILD}/eclipse" 
               preferredInstallLocation="WEB-INF/eclipse"/>
    </output-group>

    <output-group name="to_jsdoc">
       <output type="dir" 
               visibility="public" 
               platform="all" 
               location="${env.LWP_COMP_BUILD}/jsdoc" 
               preferredInstallLocation="jsdoc"/>
    </output-group>

    <!-- Reuse strings provided by these components -->
    <dependency name="lc.util/web" type="jar" />
    <dependency name="lc.widgets/lc.widgets.core" type="jar" />
    <dependency name="lc.widgets/lc.widgets.sand" type="jar" />
   
    <dependency name="lc.web.resources.base" type="dir" />
    <dependency name="lc.web.osgi" type="dir" />

    <dependency name="prereqs.sn/was7.0" type="jar" />
    <dependency name="prereqs.sn/ant-contrib" type="dir" />

    <dependency name="prereqs.sn/jasmine-2.0" type="dir" />
    <dependency name="prereqs.sn/jsdoc3" type="dir" />
    <dependency name="prereqs.sn/jslint4java" type="dir" />

    <dependency name="lc.core/web.resources.remote" type="jar" />
    <dependency name="tk.rproxysvc/server" type="jar" />
    <dependency name="tk.rproxysvc/transport" type="jar" />
    
    <dependency name="lc.shindig.serverapi/core" type="jar" />
    <dependency name="prereqs.sn/swfupload" type="file" />
    <dependency name="prereqs.sn/hystrix" type="jar" />
</component>
