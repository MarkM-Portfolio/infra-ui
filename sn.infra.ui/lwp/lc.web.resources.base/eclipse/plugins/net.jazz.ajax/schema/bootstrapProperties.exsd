   </element>

   <element name="bootstrapProperties">
      <complexType>
         <attribute name="class" type="string" use="required">
            <annotation>
               <documentation>
                  
               </documentation>
               <appInfo>
                  <meta.attribute kind="java" basedOn="net.jazz.ajax.AbstractBootstrapProperties:"/>
               </appInfo>
            </annotation>
         </attribute>
         <attribute name="id" type="string" use="required">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="priority" type="string" use="default" value="0">
            <annotation>
               <documentation>
                  The priority attibute allows multiple bootstrap property extensions to co-exist. The HIGHEST value will be used. If there are two entries that have the highest value then an arbritray choice will be made.
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

