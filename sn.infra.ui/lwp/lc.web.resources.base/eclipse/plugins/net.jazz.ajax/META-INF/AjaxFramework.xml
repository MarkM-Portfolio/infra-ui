<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0"
		name="net.jazz.ajax.internal.AjaxFramework"
		immediate="true"
		activate="activate"
		deactivate="deactivate">
	<implementation class="net.jazz.ajax.internal.AjaxFramework"/>
	<reference
			interface="com.ibm.team.jfs.app.distributed.IDistributedDataService"
			cardinality="1..1"
			name="IDistributedDataService"
			policy="static"
			bind="setDistributedDataService"
			unbind="unsetDistributedDataService"/>
	<reference
			interface="org.eclipse.osgi.service.localization.BundleLocalization"
			cardinality="1..1"
			name="BundleLocalization"
			policy="static"
			bind="setBundleLocalization"
			unbind="unsetBundleLocalization"/>
	<reference
			interface="org.osgi.service.http.HttpService"
			name="HttpService"
			cardinality="1..1"
			policy="static"
			bind="setHttpService"
			unbind="unsetHttpService"/>
</scr:component>
