<component name="appext.web.resources" 
            disabled="true"
            owner="Claudio Procida" 
            owner-email="procidac@ie.ibm.com"
            project-type="eclipse-pde">

    <!--An output group is needed to get it into the fe.zip -->
    <output-group name="to_osgi_war">
       <output type="dir" 
               visibility="public" 
               platform="all" 
               location="${env.LWP_COMP_BUILD}/eclipse" 
               preferredInstallLocation="WEB-INF/eclipse"/>
    </output-group>

    <output-group name="to_web_resources_provision">
       <output type="dir" 
               visibility="public" 
               platform="all" 
               location="${env.LWP_COMP_BUILD}/eclipse/plugins" 
               preferredInstallLocation=""/>
    </output-group>

    <output-group name="to_jsdoc">
       <output type="dir" 
               visibility="public" 
               platform="all" 
               location="${env.LWP_COMP_BUILD}/jsdoc" 
               preferredInstallLocation="jsdoc"/>
    </output-group>

	<!-- list all of your dependencies here - use the "dir" type.  
	     Most components will depend only on lc.web.osgi -->
	<dependency name="prereqs.web.resources" type="dir" />
	<dependency name="lc.web.resources.base" type="dir" />
	<dependency name="lc.web.osgi" type="dir" />
	<dependency name="lc.web.resources" type="dir" />
	<dependency name="prereqs.sn/jsdoc3" type="dir" />
	<dependency name="prereqs.sn/jslint4java" type="dir" />
</component>
